{
  "name": "identity-client",
  "version": "0.0.0",
  "description": "Angular Identity Client Application",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start:mock": "concurrently --kill-others --names \"API,WEB\" -c \"bgBlue.bold,bgGreen.bold\" \"npm run mock:api\" \"npm run start:mock:angular\"",
    "start:mock:angular": "ng serve --configuration mock",
    "mock:api": "node mock/server.js",
    "build": "ng build",
    "build:prod": "ng build --configuration production",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "lint": "eslint src/**/*.{js,ts}",
    "lint:fix": "eslint src/**/*.{js,ts} --fix",
    "format": "prettier --write src/**/*.{js,ts,html,scss,css,json}",
    "format:check": "prettier --check src/**/*.{js,ts,html,scss,css,json}"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^20.2.3",
    "@angular/cdk": "^20.2.1",
    "@angular/common": "^20.2.3",
    "@angular/compiler": "^20.2.3",
    "@angular/core": "^20.2.3",
    "@angular/forms": "^20.2.3",
    "@angular/material": "^20.2.1",
    "@angular/platform-browser": "^20.2.3",
    "@angular/platform-browser-dynamic": "^20.2.3",
    "@angular/router": "^20.2.3",
    "@jsverse/transloco": "^8.0.0",
    "@jsverse/utils": "1.0.0-beta.5",
    "@ngrx/effects": "^20.0.1",
    "@ngrx/entity": "^20.0.1",
    "@ngrx/store": "^20.0.1",
    "@ngrx/store-devtools": "^20.0.1",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular/build": "^20.2.1",
    "@angular/cli": "^20.2.1",
    "@angular/compiler-cli": "^20.2.3",
    "@types/jasmine": "~5.1.0",
    "concurrently": "^9.2.1",
    "jasmine-core": "~5.5.0",
    "json-server": "^0.17.4",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "typescript": "~5.9.2"
  }
}
